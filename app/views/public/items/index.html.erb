</header>

<h1>商品一覧</h1>
<tbody>
  <% @items.each do |item| %>
    <tr>
      <td>
        <%= image_tag item.image, size: "200x200" %>
      </td>
      <td><%= item.product_name %></td>
      <td><%= item.price %></td>
    </tr>
  <% end %>
</tbody>